<?xml version="1.0" encoding="utf-8"?>
<resources>
    
    <string name="app_name">《一键签到》猫扑MP打卡插件</string>
    <string name="label_activity_main">猫扑MP打卡插件</string>
    
    
    
    <!-- “添加任务”的弹出窗口上显示的插件名称 -->
    <string name="cfg_plugin_name">猫扑MP打卡</string>
    
    <!-- 插件作者 -->
    <string name="cfg_author">家前李树</string>
    
    <!-- “添加任务”的弹出窗口上显示的说明信息，提示用户怎么使用此插件 -->
    <string name="cfg_info">适用于猫扑MP打卡领取\n\n注意猫扑网并不是一过12点就把签到重置，有可能在凌晨1点签到时还会提示“今天已签到”，这个签到是昨天的，并不是今天的。需要再换个时间重新签到，否则签到会中断。\n不支持邮箱登录</string>
    
    <!--
    在“添加任务”的弹出窗口上显示的“配置”界面类型
    0表示不需要显示显示任何配置界面
    1表示直接由主程序提供一个简单的文本框来让用户输入数据
    2表示由插件自己来完成配置界面的高级定制
    -->
    <string name="cfg_config_type">0</string>
    
    <!-- 该任务每日可签到的次数 -->
    <string name="cfg_times">1</string>

